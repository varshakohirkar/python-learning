import random
compnum=random.randint(0,100)
while True:
   guess=input("Enter the guess number:")
   if guess>100:
         print("Not allowed")
   elif guess<1:
         print("Not allowed")
   elif guess>compnum:
         print("guess lower")
   elif guess<compnum:
         print("guess higher")
   else:
         print("You are correct")
         break

   
