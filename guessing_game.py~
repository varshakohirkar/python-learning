import random

comp=random.randint(0,100)
print(comp)
while True:
   check=input("guess should be lower or higher?")
   if str(check)=='guess lower':
      comp=random.randint(0,comp)
      print(comp)
   elif str(check)=='guess higher':
      comp=random.randint(comp,100)
      print(comp)
   elif str(check)=='correct':
      print("congrats")
      break

